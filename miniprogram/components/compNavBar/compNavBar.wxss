/* components/compNavBar/compNavBar.wxss */
.nav_mask {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1
}
.sunNavBar__wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  z-index: 10
}
.icon_arrow {
   width: 35rpx;
  height: 40rpx;
}
.sunNavBar__navBack {
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  padding-left: 28rpx;
  line-height: 60rpx;
  position: absolute;
  bottom: 0
}
.icon-emptyheart--gray {
  width: 44rpx;
  height: 44rpx;
}
.sunNavBar_heart__wrap {
  position: absolute;
  right: 230rpx;
}
.icon-emptyheart--pop {
  display: inline-block;
  height: 26rpx;
  min-width: 26rpx;
  font-size: 20rpx;
  background: #F83D3D;
  color: #fff;
  border-radius: 26rpx;
  padding: 0 10rpx;
  position: absolute;
  top: -4rpx;
  margin-left: -14rpx;
  box-sizing: border-box;
  white-space:nowrap;
}