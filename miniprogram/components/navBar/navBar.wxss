.navbar {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10086;
  flex-shrink: 0;
}

.navbar-title {
  width: 100%;
  box-sizing: border-box;
  padding-left: 115px;
  padding-right: 115px;
  font-weight:800;
  height: 32px;
  line-height: 32px;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 10;
  color: #000;
  font-size: 36rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.navbar-action-wrap {
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: absolute;
  left: 10px;
  z-index: 11;
  line-height: 1;
  padding-top: 4px;
  padding-bottom: 4px;
}

.navbar-action-group {
  border-radius: 20px;
  overflow: hidden;
}

.navbar-action_item {
  padding: 3px 0;
  color: #000;
  width: 20rpx;
  height: 35rpx;
}

.navbar-action_item_new{
  margin-left: 15rpx;
  width: 44rpx;
  height: 44rpx;
}

.navbar-action-group .navbar-action_item {
  padding: 3px 14px;
}

.navbar-action-group .last {
  width: 70rpx;
}
.navbar-search {
  position: absolute;
  width: 705rpx;
  height: 60rpx;
  background:rgba(245,245,245,1);
  border-radius:30rpx; 
  margin-left: 28rpx;
  line-height: 60rpx;
  padding-left: 30rpx;
  font-size: 24rpx;
  color: #999;
  box-sizing: border-box;
}
.icon-search {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  margin-bottom: -2rpx;
}
.navbar-text {
  margin-left: 10rpx;
  color: #999
}