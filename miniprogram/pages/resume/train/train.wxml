<view class="panel-set-list">
  <SettingItem title="机构名称" emptyText="请填写" value="{{ name }}" 
    data-url="/pages/input/input?keywords={{ name }}&title=机构名称&field=name&limit=30"
    bindtap="_navigateTo"></SettingItem>
  <SettingItem title="课程名称" emptyText="请填写" value="{{ name }}" 
    data-url="/pages/input/input?keywords={{ name }}&title=课程名称&field=name&limit=30"
    bindtap="_navigateTo"></SettingItem>
  <SettingItem title="培训时间" emptyText="请填写" value="{{ name }}" 
    data-url="/pages/input/input?keywords={{ name }}&title=培训时间&field=name&limit=30"
    bindtap="_navigateTo"></SettingItem>
</view>

<button class="btn btn-blue save-btn">保存</button>