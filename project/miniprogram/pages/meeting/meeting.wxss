/* pages/meeting/meeting.wxss */
page {
  height: 100%;
  background: #000
}
.fixed {
  position: fixed;
  top: 0;
  z-index: 1000;
}
.content {
  width: 100%;
  position: absolute;
}
.video-item0 {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.back_item {
width: 44rpx;
height: 44rpx;
position: absolute;
left: -50rpx;
top: 2rpx;
}
.content .footer {
  width: 100%;
  font-size: 24rpx;
  justify-content: flex-end;
  height: 130rpx;
  background: rgba(0,0,0,.35);
  position: fixed;
  bottom: 0;
  z-index: 10
}

.content .footer .toolbar {
  /* border-top: 1px solid #A5A5A5; */
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.content .footer .mic.btn {
  background-image: url("data:image/png;base64,");
}

.content .footer .mic.muted {
  background-image: url("data:image/png;base64,")
}

.content .footer .btn {
  height: 66rpx;
  width: 66rpx;
  background-size: 100% 100%;
  border: 0;
  background-position: center;
  background-repeat: no-repeat;
}

.content .footer .log.hover{
  background-image: url('data:image/png;base64,');
}

.content .footer .log {
  background-size: contain;
  background-image: url('data:image/png;base64,');
}


.content .footer .hangup {
  background-image: url("data:image/png;base64,")
  }

.content .footer .abled_camera{
  background-image: url("data:image/png;base64,")
  }
.content .footer .unpubish {
   background-image:url("data:image/png;base64,");
}
.content .footer .camera{
  background-image:url("data:image/png;base64,")
  }

 .makeup{
  background-image:url('data:image/png;base64,');
}

.makeup.off{
  background-image: url('data:image/png;base64,');
}

.content .video-container {
  display: flex;
  width: 100%;
  overflow: hidden;
  flex-wrap: wrap;
}
.video_channel {
position: absolute;
top: 66rpx;
left: 74rpx;
height: 50rpx;
background: rgba(0,0,0,0.2);
border-radius: 25rpx;
z-index: 10;
color: #fff;
padding: 0 20rpx;
font-size: 22rpx;
line-height: 50rpx;
}
.touchView {
  z-index: 10
}
.localView {
  z-index: 0;
}
.set_beauty {
  position: fixed;
  top: 200rpx;
  right: 30rpx;
  height: 80rpx;
}
.beauti_btn {
  display: inline-block;
  width: 40rpx;
  background-size: 100% 100%;
  height: 56rpx;
  bacground-color: none;

}